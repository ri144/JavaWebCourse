
public class TransactionSummary extends Receipt{
/*	+--------------------------------------+
	|      Java Bank ATM Receipt           |
	|      Wednesday, December 2, 2015     |
	|      ATM Location # 123              |
	|                                      |
	|                                      |
	|      Account Number:      1234567    |
	|      Customer:     John Q. Public    |
	|      Transaction Type:    Deposit    |
	|      Transaction Amount:  $500.00    |
	|      Account Balance:   $1,500.00    |
	|                                      |
	|      Thank you for banking with us   |
	|            Have a coffee day         |
	|                                      |
	+--------------------------------------+*/
	@Override
	public String printReceipt(String name, int actNum, String type, float amount, float total, String day, int month, int year){
		String complete;
		complete = "+--------------------------------------+\n"
				+ "|      Java Bank ATM Receipt           |\n"
				+ "|      " + day + ", " + numToStr(month);
		return complete;		
	}

}
